<%if !collect_node_up.edges_as_sink.empty?%>
  <% collect_node_up.edges_as_sink.each do |edge|  %>
    <%if !@edges_array.include?(edge)%>      
      <%@edges_array[@j] = edge%>
      <%@j=@j+1%>
      <%if !@nodes_array.include?(edge.source)%>
        <%@i=@i+1%>
        <%@nodes_array[@i] = edge.source%>
        <%= render :partial => "collect_node_up", :object => edge.source %>
      <%end%>
    <%end%>
  <%end%>
<%end%>